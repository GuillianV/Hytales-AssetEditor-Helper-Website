{
	"name": "cms",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"initialize": "node initialize.js",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "pnpm run initialize && vite build && node server.js",
		"startup": "pnpm i && pnpm add prisma --save-dev && npx prisma generate && npx prisma migrate deploy && pnpm run initialize && pnpm run build && node server.js",
		"prettify": "npx prettier --write .",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "npx prettier --write . && prettier --check . && eslint .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@editorjs/editorjs": "^2.31.1",
		"@editorjs/header": "^2.8.8",
		"@editorjs/image": "^2.10.3",
